<?xml version="1.0" encoding="UTF-8"?>
<AnalysisSetting>

    <!-- Simulation Setting -->
    <TimeStep type="1" dt="0.016"></TimeStep>
    <Gravity x="0.0" y="0.0"></Gravity>
    <Particles nMaxParticles="100000" maxParticleSize="0.05"></Particles>
    <TensileInstability k="0.001" dq="0.1" n="4.0"></TensileInstability>
    <AnalysisBox startX="0.0" startY="0.0" endX="10.0" endY="10.0"></AnalysisBox>
    
    <!-- Propoerty Setting -->
    <PhaseProperties number="1">
        <phase type="Fluid" name="water" size="0.05" density="1000.0" viscosity="0.001"> </phase>
        <!-- <phase type="Solid" name="rock" size="0.001" density="2650"> </phase> -->
    </PhaseProperties>

    <!-- Particle Setting -->
    <InitialBox number="2">
        <!-- 초기유속 넣는 것도 작업해야함 -->
        <initBox phaseID="1" startX="1.0" startY="2.0" endX="2.0" endY="3.0"></initBox>
    </InitialBox>

    <!-- Boundary Setting -->
    <Boundaries number="3">
        <Line startX="0.0" startY="0.0" endX="10.0" endY="10.0"></Line>
        <Line startX="0.0" startY="0.0" endX="10.0" endY="10.0"></Line>
        <Box startX="0.0" startY="0.0" endX="10.0" endY="10.0"></Box>
    </Boundaries>
    
</AnalysisSetting>